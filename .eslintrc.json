{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "react-app",
    "plugin:import/errors",
    "plugin:import/warnings",

    // import 관련 규칙 모음

    "plugin:prettier/recommended",
    "prettier"
    // prettier 관련 규칙 모음
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "prettier/prettier": ["error", { "endOfLine": "auto" }]
  }
}
